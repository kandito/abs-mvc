// Write uTVL feature model here
root IBFeatures {
    group allof {
        Login,
        Account {
            group allof {
                Deposit,
                Withdraw {
                    group allof {
                        opt Fee {Int fee in [1..10000]; },
                        opt CheckBalance {Int min in [1..1000000]; }
                    }
                }
            }
        },
        opt ContactPage
    }
}
